<p id="notice"><%= notice %></p>

<h1>Item Not Includeds</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Description</th>
      <th>Trip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_not_includeds.each do |item_not_included| %>
      <tr>
        <td><%= item_not_included.item %></td>
        <td><%= item_not_included.description %></td>
        <td><%= item_not_included.trip_id %></td>
        <td><%= link_to 'Show', item_not_included %></td>
        <td><%= link_to 'Edit', edit_item_not_included_path(item_not_included) %></td>
        <td><%= link_to 'Destroy', item_not_included, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item Not Included', new_item_not_included_path %>
